{
  "version": 2,
  "builds": [
    {
      "src": "/index.js",  // No leading slash
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",  // No leading slash
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
